import numpy as np

np.random.seed(12)

n = 0

# set n to whatever value is called for in the question

def calculateFraction(n):
  if (n % 2 == 0): # even
    s = np.random.uniform(0,(n/2)/n,100)
    return np.mean(s)
  else: # odd
    s = np.random.uniform(0,((n+1)/2)/n,100)
    return np.mean(s)

calculateFraction(n)

# output sensitive even with 10^8 iterations
